<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.blog.dao.DiscussDao">
    <select id="findDiscussUser" resultType="com.blog.model.DiscussCustom">
        SELECT discuss.*, user.username FROM discuss,user WHERE discuss.userId=user.uid
    </select>


</mapper>